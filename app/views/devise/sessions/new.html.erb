<nav class="objects-header">
<h2>Log In</h2>
</nav>
<div class="container">
<div class="row">
<div class="col-3">
</div>
<section class="object-menu col-6">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <p>In this demo you may Log In with the username johndoe</p>
    <%= f.label :name %><br />
    <%= f.text_field :name, :value => 'johndoe', id: 'name', autocomplete: "name" %>
  </div>

  <%= f.hidden_field :password, :value => 'password', autocomplete: "password" %>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>
<%= render "devise/shared/links" %>
</section>
<div class="col-3">
</div>
</div>
</div>

