<p id="notice"><%= notice %></p>

<h1>Grouped Projects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grouped_projects.each do |grouped_project| %>
      <tr>
        <td><%= link_to 'Show', grouped_project %></td>
        <td><%= link_to 'Edit', edit_grouped_project_path(grouped_project) %></td>
        <td><%= link_to 'Destroy', grouped_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grouped Project', new_grouped_project_path %>
